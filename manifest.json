{
  "manifest_version": 2,

  "name": "FancyTab",
  "description": "Beautiful photos hand-selected by Flickr's danielpuglisi for your New Tab",
  "version": "1.0.0",

  "permissions": [
    "http://api.flickr.com/",
    "tabs"
  ],

  "chrome_url_overrides": {
    "newtab": "flickrset.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
  "icons": {"48": "icon48.jpg",
            "128": "icon128.jpg"}
}
